@using AutoRest.Swift
@using AutoRest.Core.Model
@using AutoRest.Swift.Templates
@using AutoRest.Swift.Model
@using AutoRest.Core.Utilities
@using System.Linq

@inherits AutoRest.Core.Template<AutoRest.Swift.Model.PropertySwift>
        @if (this.Model.ModelType.CanBeEmpty())
    {
@:@($"var {this.Model.VariableName}: {this.Model.ModelType.Name}");        
    }
    else
    {
@:@($"let {this.Model.VariableName} = {this.Model.ModelType.DefaultValue}");
    }