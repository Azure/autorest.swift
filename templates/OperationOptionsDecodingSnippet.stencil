{% if strategy == "dateFromParam" %}
let {{ key }}String = DateFormatter().string(from: {{ key }})
{% elif strategy == "dateTimeFromParam" %}
let {{ key }}String = Date.Format.iso8601.formatter.string(from: {{ key }})
{% elif strategy == "byteArrayFromParam" %}
let {{ key }}String = String(bytes: {{ key }}, encoding: .utf8)
{% else %}
{# No need to decode the parameter to String. Leave this blank #}
{% endif %}

