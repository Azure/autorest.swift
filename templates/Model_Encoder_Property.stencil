{% if property.isDate %}
    {% include "Model_Encoder_DateProperty.stencil" %}
{% else %}
    {% if property.optional %}
        if {{ property.name }} != nil { try? container.encode({{ property.name }} , forKey: .{{ property.name }}) }
    {% else %}
        try container.encode({{ property.name }}, forKey: .{{ property.name }})
    {% endif %}
{% endif %}
